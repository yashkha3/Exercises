def student_data(sno, *args,**kwargs):
    for key,val in kwargs.items():
        if kwargs['year'] < 2000:
            print (val, "is ", key)
        elif kwargs['year'] >  2010:
            print ("I am to bigggggg")
        else:
            print ("In else")

student_data("202050", "Rani", "Bod", age=25, year=2008, address="Vijay naagr", Mobile="9826030")